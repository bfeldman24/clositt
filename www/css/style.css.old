
/*** GENERAL ***/
html,
body {
	margin:0;
	padding:0;
	height:100%;
}

body{	    
	padding-left: 0px;
    padding-right: 0px;
    font-family: 'Roboto', sans-serif ;
}

p, div, span, input, button, select, textarea, a, td {
	font-family: 'Roboto', sans-serif ;
}

a{
	cursor: pointer;	
}

.width100{
	width: 100%;
}		

a:hover{
	color:#71C5EF;
}

textarea{
 	resize: none;
    width: 100%;
}

/*** TOP NAV ***/

.logo-beta{
   font-size: 15px;
   font-style: italic;
   font-variant: small-caps;   
}

.navbar-fixed-top{    
    border-width: 0;
    height: 50px;
	margin: 0;
	min-width: 100%;
}

#brand{
    font-weight: 700; 
    color: #00CC99;
}

.navbar-default .navbar-brand:hover, .navbar-default .navbar-brand:focus {
    color: #FFFFFF;
}

#user-dropdown{
    height: 40px;
    right: 0px;
    margin-left: 100px;
    margin-right: 100px; 
}

.navbar, .navbar .collapse.in{
    background: RGBA(255,255,255,.85);
    box-shadow: 15px 2px 7px 0 rgba(77, 77, 77, 0.75);
}

.navbar .top-nav-wrapper .container{
	width:100%;	
}

.navbar .brand {		
	float: left;		  
	margin-right: 20px;		
	color: #FFFFFF;	  
}

.navbar .nav > li > .dropdown > span.dropdown-toggle {  
	color: #FFFFFF;
	display: block;  
	float: none;  
	padding: 10px 15px;  
	text-decoration: none;  
    text-shadow: 0 2px 0 #333333;	
}

.navbar .nav > li > .dropdown > span.dropdown-toggle > .caret{
 	border-top: 4px solid #A4A4A4;
 }
 
 .navbar .nav > li > .dropdown > span.active > .caret{
 	border-top: 4px solid #00FF00;
 }

.navbar .nav > li > a{
    color: #555555;
    font-size: 18px;    
}

.navbar .nav > li:hover{
    color: #FFFFFF;
    background: #00CC99;
}

.navbar i.browse-small-icon, .navbar i.myclositt-small-icon{
    display: inline-block;
    height: 18px;
    margin-right: 5px;
    position: relative;
    top: 2px;
    width: 23px;
}

.navbar i.browse-small-icon{
    background: url("images/browse-small.png") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
}

.navbar i.myclositt-small-icon{   
    background: url("images/hanger2-small.png") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
}

.navbar-nav.navbar-right:last-child{
	margin-right: 0px;
}

.navbar #loginBtns > li > a {     
    color: #00CC99;
    font-size: 14px; 
}

.navbar .nav > li > a:hover, .navbar #loginBtns > li > a:hover {     
    color: #FFFFFF;
}
 
.navbar a.btn, .navbar a.navbar-link{
       padding: 4px 14px;          
       margin: 10px 5px;
}

.navbar a.btn{
    border: 1px solid #00CC99;
    background: #FFFFFF;
    border-radius: 5px;
    font-weight: bold;
    box-shadow: none;		
}

.navbar .nav > li.loggedoutBtns:hover{
    background: transparent;
}

.navbar #loginBtns > li > a.btn:hover{
    color: #FFFFFF;
    background: #00CC99;    
    text-decoration: none;
}

.navbar #loginBtns a.btn.inverse{
    background: #00CC99;   
    color: #FFFFFF;
}

.navbar #loginBtns a.btn.inverse:hover{
    color: #F5F5F5;
    background: #008E6B;   
}

.toggle-filter-nav.navbar-toggle {
    padding-left: 15px;
    padding-right: 15px;   
}


/*** TOP SUB NAV ***/	

.top-sub-nav{
	top:40px;
	border-top:1px solid #00FF00; 	
	 z-index: 1000;
}

.top-sub-nav .nav > li.sub-nav-add{
	font-size: 30px;		
}

.top-sub-nav .nav > li.sub-nav-add a:hover{
	color: #00CD00;	
}

.top-sub-nav .navbar-inner{
	border-radius: 0 0 14px 14px;
	border-top:none;	
}

.top-sub-nav .nav > .active > a, .top-sub-nav .nav > .active > a:hover, .top-sub-nav .nav > .active > a:focus {			
	border-radius: 5px 5px 5px 5px;		
}

a.notimplemented{
	opacity: 0.6;
	filter:alpha(opacity=60); /* For IE8 and earlier */	
}

.hero-unit h2, .hero-unit #login-alert{
	text-align:left;	
}

.hero-unit form{
	margin: 30px auto;	
}

/*** SIDE NAV ***/

.bs-docs-sidenav{	
	margin-top: 60px;
}

.icon-chevron-down{
	float: right;			
	margin-right: -6px;			
	margin-top: 2px;			
	opacity: 0.25;	
}

.nav-dropdown-tab{
	border: 1px solid rgb(229, 229, 229); 
	padding: 8px 14px; 
	margin: 0px 0px -1px; 
	height: 200px; 
}

		

/*** MAIN CONTAINER ***/

@media (min-height: 0px) and (max-height: 500px) {
  .main-container {
    min-height: 475px;
  }
}

@media (min-height: 501px) and (max-height: 750px) {
  .main-container {
    min-height: 525px;
  }
}

@media (min-height: 751px) and (max-height: 1000px) {
  .main-container {
    min-height: 675px;
  }
}

@media (min-height: 1001px) and (max-height: 1250px) {
  .main-container {
    min-height: 775px;
  }
}

@media (min-height: 1251px) and (max-height: 1500px) {
  .main-container {
    min-height: 875px;
  }
}

@media (min-height: 1501px) and (max-height: 1750px) {
  .main-container {
    min-height: 975px;
  }
}

@media (min-height: 1751px) and (max-height: 9000px) {
  .main-container {
    min-height: 1075px;
  }
}

.main-container{
	padding: 0 0px;	
	height:100%;	
	margin:20px auto;
	width:100%;
}		

.main-container .page-header{
	padding: 0 20px;	
}

/*** MODAL ***/ 
.modal-footer {
    text-align: center !important;
}

.modal-footer > button, .modal-footer .btn + .btn {
    margin-bottom: 5px;
}   

.modal-body {
    font-size: 18px;
    text-align: center;
}

/*** FOOTER ***/

footer{
	width: 100%;
	margin-top: 70px;
   	padding:50px 20px;

	border-top:1px solid #00FF00;
	
	-moz-box-shadow: 0px 1px 5px #888;
	-webkit-box-shadow: 0px 1px 5px #888;
	box-shadow: 0px 1px 5px #888;
	
}

footer p{
	color: #BABABA;			
	margin-bottom: 0;
}	

footer{
	position:fixed;
	bottom:0;
	height: 30px;
	color: #FFFFFF;
	border-top: none;
    box-shadow: 0 1px 5px #888888;
    margin-top: 0px;
    padding: 0px;
    z-index: 9999;			
	
}

footer .footer-item{
	float:left;
	position:relative;
	left: -50%;
	padding: 5px 10px;
	/* border-left: 1px solid #009e5c;	*/
}

footer a{
	color:#FFFFFF;
}

footer a:hover{
	/* background-color: #14B770; */
	color: #FFFFFF;		  
	border-radius: 5px 5px 5px 5px;	
}

footer > #footer-filter{
	cursor: pointer;
    float: left;
    height: 100%;
    padding: 7px 10px;
}

footer > #footer-filter .icon-chevron-down{
	opacity: 1;	
	float: none;
	margin:0;
}
	
footer > .center{
	margin-left:20%;	
}

/* 
footer .last{
	border-right: 1px solid #009e5c;	
}
*/

#footer {
	width:100%;
	height:80px;
	position:absolute;
	bottom:0;
	left:0;
	background:#ee5;
}

#footer-wrapper{
	position: relative; 
	float: left; 
	left: 50%;
}

.feedback{
    bottom: 3px;
    position: fixed;
    right: 5px;
    width: 145px;  
    opacity: 0.3; 
    color: #333333;
}

.feedback:hover{
   opacity: 1;  
}

.feedback-popup{
    background: none repeat scroll 0 0 #00CC99;
    bottom: 45px;
    height: 105px;
    position: absolute;
    right: 5px;
    width: 145px;
    border-radius: 8px;
}

.feedback-popup > .feedback-textarea {
    border: medium none;
    margin: 0 10px;
    padding: 2px;
    position: absolute;
    top: 10px;
    width: 125px;
}

.feedback .arrow-down {
	border-left: 20px solid rgba(0, 0, 0, 0);
    border-right: 20px solid rgba(0, 0, 0, 0);
    border-top: 20px solid #00CC99;
    bottom: 28px;
    height: 0;
    position: absolute;
    right: 20px;
    width: 0;
    z-index: 999;
}

.feedback .feedback-submit-btn, .feedback .feedback-minimized-btn{
    bottom: 1px;
    position: absolute;
    right: 15px;   
}

.feedbackMinimize {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 4px solid #00CC99;
    border-radius: 15px;
    bottom: 132px;
    color: #666666;
    cursor: pointer;
    font-weight: bold;
    height: 23px;
    padding: 0 0 2px;
    position: absolute;
    right: -1px;
    width: 23px;
}

.feedbackMinimize:hover {
    background: none repeat scroll 0 0 #DFDFDF;
}

.feedbackMinimize:active {
    background: none repeat scroll 0 0 #BFBFBF;
}

.minimize {
    left: 5px;
    position: relative;
    top: -4px;   
}

