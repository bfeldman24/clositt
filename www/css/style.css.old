/**** MEDIA QUERIES ****/

/* XXS Mobile */
@media (max-width: 450px) {
    .col-xs-1, .col-xs-3, .col-xs-5, .col-xs-7, .col-xs-9, .col-xs-11, .col-xs-12{
        float:none;
        margin: 0 auto 25px !important;   
    }   
}

/* XS Mobile */
@media (max-width: 767px) {
    .overlay{
            display: block !important; 
    }
    
    #productModal .modal-content{
        padding: 0;   
    }
      
    #filter-float-container, #filter-float-container.affix{
        width: 100%;   
        opacity: 1;
        background: none repeat scroll 0 0 rgba(255, 255, 255, 1);
        height: 100%;
        overflow: hidden;
        z-index: 1000;
        left: 0;
        top:50px;        
        position: fixed !important;
    }
    
    #filter-float h5, #feedSettings-float h5{
       font-size: 22px;
   }        
    
    #filter-float h4, #feedSettings-float h4{
        font-size: 24px;
    }
    
    #filter-float .checkbox, #feedSettings-float .checkbox{
        font-size: 22px;
        line-height: 28px;
        width: 200px;
    }
    
    #filter-float .checkbox:hover, #feedSettings-float .checkbox:hover{
        background: none repeat scroll 0 0 #EFEFEF;   
    }
    
    #filter-float input[type="checkbox"], #feedSettings-float input[type="checkbox"]{
        width: 20px;
        height: 20px;   
        margin-top: 7px;
    }
    
    #filter-float .filterValueName, #feedSettings-float .filterValueName{
        margin-left: 10px;   
    }
    
    #selectedFilters .selectedFilter-wrapper {
        font-size: 26px;
        line-height: 30px;
        height: 30px;
    }
    
    #selectedFilters .selectedFilter-wrapper .selectedFilter-x{
        padding: 0 10px;
        font-size: 90%; 
        border-radius: 26px;  
    }
       
}

